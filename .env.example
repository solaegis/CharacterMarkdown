# ESOUI Credentials for Automated Upload
# Copy this file to .env and fill in your values
# DO NOT commit .env to version control!

# Your ESOUI account username
ESOUI_USERNAME=your_username_here

# Your ESOUI account password
ESOUI_PASSWORD=your_password_here

# Your addon ID (get from addon URL after first upload)
# Example: https://www.esoui.com/downloads/info1234.html -> ID is 1234
ESOUI_ADDON_ID=

# Optional: Run browser in headless mode (true/false)
HEADLESS=false
